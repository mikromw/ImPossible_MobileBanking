<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.androidtutorialshub.loginregister">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/title_app"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SplashScreen"
            android:noHistory="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" />
        <activity
            android:name=".HomeUserActivity"
            android:label="@string/app_name1"
            android:screenOrientation="portrait" />
        <activity
            android:name=".TransferActivity"
            android:label="@string/app_name2"
            android:screenOrientation="portrait" />
        <activity
            android:name=".PulsaActivity"
            android:label="@string/app_name3"
            android:screenOrientation="portrait" />
        <activity
            android:name=".NoHpActivity"
            android:label="@string/app_name4"
            android:screenOrientation="portrait" />
        <activity
            android:name=".InfoUserActivity"
            android:label="@string/app_name5"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ProfileUserActivity"
            android:label="@string/app_name6"
            android:screenOrientation="portrait" />

        <activity android:name=".SecurityActivity"
            android:label="@string/app_name7"
            android:screenOrientation="portrait"/>
        <activity android:name=".HistoryActivity"
            android:label="@string/app_name8"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>